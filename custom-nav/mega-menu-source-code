<script>
$(document).ready(function(){$(".menu > ul").before('<a href="#" class="menu-mobile"> </a>');$(".menu > ul > li:has( > ul)").addClass("menu-dropdown-icon");$(".menu > ul > li > ul:not(:has(ul))").addClass("normal-sub");$(".hover-this-menu").hover(function(f){if($(window).width()>943){$(this).children("ul").stop(true,false).fadeToggle(150);f.preventDefault()}});var b=$(".mega-menu"),c=$(".menu-mobile"),a=$(".menu > ul > li"),d=$(".menu > ul > li > ul > li");c.click(function(f){if($(window).width()<=943){f.preventDefault();$(".menu > ul").toggleClass("show-on-mobile");c.toggleClass("active");b.toggleClass("active")}});a.click(function(f){f.preventDefault();$(this).toggleClass("left-slide").next("ul").toggleClass("left-slide");return false});d.click(function(f){f.preventDefault();$(this).toggleClass("level-3-nav").next("ul").toggleClass("level-3-nav");return false});$(".menu > ul > li > ul > li > ul > li > a").click(function(f){f.stopImmediatePropagation();return true});$("a.logo").click(function(f){f.stopImmediatePropagation();return true});$(".menu > ul > li > ul.normal-sub > li > a").click(function(f){f.stopImmediatePropagation();return true});$(".menu > ul > li.no-submenu > a").click(function(f){f.stopImmediatePropagation();return true})});(function(b){var c=b(window);function a(){if(c.width()<943){b(".shop-by-house").insertAfter(".mega-menu li:nth-child(7)")}else{b(".shop-by-house").insertAfter(".before-shop-by-house");b(".menu > ul > li > ul > li > a").click(function(d){d.stopImmediatePropagation();return true});b(".menu > ul > li.no-submenu > a").click(function(d){d.stopImmediatePropagation();return true})}}c.resize(a).trigger("resize")})(jQuery);
</script>
